{
  "videos": [
    {{#repeat min=10 max=20}}
    {
      "id_video": "v{{@index}}",
      "id_user": "u{{int 0 14}}",
      "date": "{{date '2024-10-07' '2025-11-07' 'DD/MM/YYYY HH:mm:ss'}}",
      "title": "{{lorem min=3 max=8}}",
      "desc": "{{lorem min=10 max=40}}",
      "path": "/videoTest.mp4"  
    }
    {{/repeat}}
  ],
  "users": [
    {{#repeat min=10 max=15}}
    {
      "id_user": "u{{@index}}",
      "pseudo": "{{firstName}} {{lastName}}",
      "avatar": "{{#if (gt (int 0 1) 0)}}/avatars/avatar{{int 1 10}}.jpg{{else}}/default.png{{/if}}", 
      "desc": "{{lorem min=10 max=30}}",
      "email": "{{email}}",
      "password": "{{lorem min=6 max=12}}",
      "subscribers": {{int 0 5000}}
    }
    {{/repeat}}
  ],
  "comments" : [
    {{#repeat min=10 max=20}}
    {
      "id_comment": "c{{@index}}",
      "id_user": "u{{int 0 14}}",
      "id_video": "v{{int 0 39}}",
      "date": "{{date '2024-10-07' '2025-11-07' 'DD/MM/YYYY HH:mm:ss'}}",
      "content": "{{lorem min=5 max=25}}"
    }
    {{/repeat}}
  ]
}